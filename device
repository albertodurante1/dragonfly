class Device {
  constructor(name, data) {
    this.name = name;
    this.dati = dati;  
    this.topicListInput = [];
    this.topicListOutput = [];
  }

  getName(){
    return this.name;
  }

  getDati(){
    return this.dati;
  }
  getTopic(){
    return this.topicListInput;
  }
  getTopic(){
    return this.topicListOutput;
  }
}

class Topic
{
constructor(nome,options)

  getId(){
  return this.nome;
  }

  getOption(){
    return this.options;
  }
}
 

module.exports = {Device,Topic};
