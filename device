class Device {
  constructor(name, topicListInput,topicListOutput) {
    this.name = name;
    this.topicListInput = topicListInput;
    this.topicListOutput = topicListOutput;
    this.smaster;

  }

  getName(){
    return this.name;
  }

  getTopic(){
    return this.topicListInput;
  }
  getTopic(){
    return this.topicListOutput;
  }

  getsmaster(){
    return this.smaster;
  }
}

class Topic
{
constructor(nome,options) //option Ã© una lista

  getId(){
  return this.nome;
  }

  getOption(){
    return this.options;
  }
}
 

module.exports = {Device,Topic};
